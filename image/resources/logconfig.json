{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters": {
    "default": {
      "()": "uvicorn.logging.DefaultFormatter",
      "format": "[%(asctime)s] [%(process).4d] [%(levelname).4s] %(message)s",
      "datefmt": "%Y-%m-%d %H:%M:%S %z"
    },
    "access": {
      "()": "uvicorn.logging.AccessFormatter",
      "format": "[%(asctime)s] [%(process).4d] [%(levelname).4s] HTTP: %(client_addr)s - %(request_line)s - %(status_code)s",
      "datefmt": "%Y-%m-%d %H:%M:%S %z"
    }
  },
  "handlers": {
    "default": {
      "formatter": "default",
      "class": "logging.StreamHandler",
      "stream": "ext://sys.stderr"
    },
    "access": {
      "formatter": "access",
      "class": "logging.StreamHandler",
      "stream": "ext://sys.stdout"
    }
  },
  "loggers": {
    "uvicorn": {
      "handlers": [
        "default"
      ],
      "level": "INFO",
      "propagate": false
    },
    "uvicorn.error": {
      "handlers": [
        "default"
      ],
      "level": "INFO",
      "propagate": false
    },
    "uvicorn.access": {
      "handlers": [
        "access"
      ],
      "level": "INFO",
      "propagate": false
    }
  }
}